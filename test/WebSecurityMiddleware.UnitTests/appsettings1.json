{
  "AppSecurity": {
    "HttpResponseHeaders": {
      "Add": {
        "X-Content-Type-Options": "nosniff",
        "Strict-Transport-Security": "max-age=1800; includeSubDomains; preload"
      },
      "Remove": [
        "X-Powered-By",
        "X-AspNet-Version",
        "X-AspNetMvc-Version"
      ]
    }
  }
}
