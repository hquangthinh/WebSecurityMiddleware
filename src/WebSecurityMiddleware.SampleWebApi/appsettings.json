{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AppSecurity": {
    "HttpResponseHeaders": {
      "Add": {
        "X-Content-Type-Options": "nosniff",
        "Strict-Transport-Security": "max-age=3600; includeSubDomains; preload"
      },
      "Remove": [
        "X-Powered-By",
        "X-AspNet-Version",
        "X-AspNetMvc-Version"
      ]
    }
  }
}
